	HostName pineman.eu
    LogLevel quiet
    # Router
    LocalForward 8080 192.168.1.254:80
    # Traefik ingress
    LocalForward 8081 localhost:8080
    # Transmission
    LocalForward 9091 localhost:9091
    # Grafana
    LocalForward 3000 localhost:3000
    LocalForward 9090 localhost:9090
